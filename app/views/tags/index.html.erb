<h1>Tags#index</h1>
<p>Find me in app/views/tags/index.html.erb</p>
<%= form_tag(search_path, method: :get) do %>
  <%= text_field_tag :term, params[:term] %>
  <%= submit_tag 'Search', name: nil %>
<% end %>
<% @tags.each do |tag| %>
  <div class='tag'>
    <%= image_tag(tag.photo.image.thumb.url, alt: 'Image') %>
  </div>
<% end %>
